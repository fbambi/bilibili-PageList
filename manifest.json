{
    "manifest_version": 2,
    "name": "bilibili-pageList",
    "description": "Bilibili 多P列表插件",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon128.png"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.bilibili.com/video/*"
            ],
            "js": [
                "./main/dist/js/manifest.js",
                "./main/dist/js/vendor.js",
                "./main/dist/js/app.js"
            ]
        }
    ]
}